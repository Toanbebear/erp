<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <record id="wizard_change_booking_date" model="ir.ui.view">
            <field name="name">Cập nhập ngày hẹn lịch</field>
            <field name="model">change.booking.date</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <field name="booking_id" readonly="True" force_dave="True"/>
                            <field name="booking_date" required="1"/>
                        </group>
                    </sheet>
                    <footer>
                        <button name="confirm" type="object" string="Xác nhận" class="oe_highlight"/>
                    </footer>
                </form>
            </field>
        </record>
    </data>
</odoo>