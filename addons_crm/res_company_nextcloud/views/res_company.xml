<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <record id="view_res_company_url_nextcloud" model="ir.ui.view">
            <field name="name">view.res.company.url.nextcloud</field>
            <field name="model">res.company</field>
            <field name="inherit_id" ref="base.view_company_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='website']" position="after">
                    <field name="url_image" string="Link ảnh của khách hàng"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>