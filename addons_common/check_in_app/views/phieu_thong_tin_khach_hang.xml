        <template id="checkin_document">
            <t t-call="web.html_container">
                <t t-call="web.external_layout">
                    <t t-set="o" t-value="o.with_context(lang=o.sudo().partner.lang)"/>
                    <t t-set="o" t-value="o.sudo()"/>

                    <div class="page" style="font-family:Times New Roman;font-size:25px;">
                        <div class="oe_structure"/>


                        <style>
                            .table_1 {
                            text-align: center;
                            vertical-align: middle;
                            padding: 10px;
                            width="100%";
                            font-size:40px;
                            }

                            .table_1 td {
                            border: 1px solid black;
                            }
                        </style>
                        <div>
                            <t t-if="not o.booking.qr_code_id">
                                <table class="table_1" width="100%" style="text-align: center;">
                                    <tr>
                                        <td class="p-2">
                                            <t t-if="o.booking.brand_id.code == 'KN'">
                                                <img src="/check_in_app/static/description/logo_kn.png"
                                                     style="width:150px"/>
                                            </t>
                                            <t t-elif="o.booking.brand_id.code == 'PR'">
                                                <img src="/check_in_app/static/description/logo_pr.png"
                                                     style="width:150px"/>
                                            </t>
                                            <t t-else="">
                                                <t t-esc="o.booking.brand_id.code"/>
                                            </t>
                                        </td>
                                        <td>PHIẾU THÔNG TIN KHÁCH HÀNG</td>
                                    </tr>
                                </table>
                            </t>
                            <t t-else="">
                                <table class="table_1" width="100%" style="text-align: center;">
                                    <tr>
                                        <td class="p-2">
                                            <t t-if="o.company_id.brand_id.code == 'KN'">
                                                <img src="/check_in_app/static/description/logo_kn.png"
                                                     style="width:150px"/>
                                            </t>
                                            <t t-elif="o.company_id.brand_id.code == 'PR'">
                                                <img src="/check_in_app/static/description/logo_pr.png"
                                                     style="width:150px"/>
                                            </t>
                                            <t t-esc="o.booking.brand_id.code"/>
                                        </td>
                                        <td class="text-center">
                                            <div>PHIẾU THÔNG TIN</div>
                                            <div>KHÁCH HÀNG</div>
                                        </td>
                                        <td class="p-2">
                                            <img t-if="o.booking.qr_code_id" width="150px" style="width:150px"
                                                 t-att-src="image_data_uri(o.booking.qr_code_id)"
                                            />
                                        </td>
                                    </tr>
                                </table>


                            </t>
                        </div>
                        <br/>
                        <br/>
                        <div class="row">
                            <div class="col-8">Họ và tên:
                                <strong>
                                    <span t-esc="o.name"/>
                                </strong>
                            </div>
                            <div class="col-4" style="text-align: right;">
                                <t t-if="o.booking">
                                    <span>
                                        <b>
                                            <t t-esc="o.booking.name"/>
                                        </b>
                                    </span>
                                </t>
                                <t t-else="">
                                    Điện thoại:
                                    <b>
                                        <span t-esc="o.phone"/>
                                    </b>
                                </t>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-6">Ngày sinh:
                                <strong>
                                    <t t-if="o.date_of_birth">
                                        <span>
                                            <span t-esc="o.date_of_birth.strftime('%d/%m/%Y')"/>
                                        </span>
                                    </t>
                                    <t t-elif="o.year_of_birth">
                                        <span>
                                            <span t-esc="o.year_of_birth.strftime('%d/%m/%Y')"/>
                                        </span>
                                    </t>
                                    <t t-else="">
                                        <span>
                                            ......................
                                        </span>
                                    </t>
                                </strong>
                            </div>
                            <div class="col-6" style="text-align: right;">
                                Checkin:
                                <b>
                                    <span t-esc="(o.create_date + relativedelta(hours=7)).strftime('%H:%M %d/%m/%Y')"/>
                                </b>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-12">Nhóm dịch vụ quan tâm:
                                <strong>
                                    <span t-esc="', '.join(o.service_category_ids.mapped('name'))"/>
                                </strong>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-12">Mong muốn:
                                <strong>
                                    <t t-if="o.desire == 'tu_van'">
                                        <span>Tư vấn</span>
                                    </t>
                                    <t t-elif="o.desire == 'lam_luon'">
                                        <span>Làm luôn</span>
                                    </t>
                                    <t t-elif="o.desire == 'event'">
                                        <span>Tham gia sự kiện</span>
                                    </t>
                                    <t t-else="">
                                        <span>
                                            ..............................
                                        </span>
                                    </t>
                                </strong>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-12">Thông tin tư vấn:</div>
                        </div>
                        <br/>
                        <div class="row">
                            <div class="col-12" style="white-space: nowrap;
                                              width: auto;
                                              overflow: hidden;
                                              text-overflow: ellipsis;">
                                <!--                        <div>-->
                                <!--                            <t t-esc="'Dịch vụ chi nhánh tư vấn:' + '.'*400"/>-->
                                <!--                        </div>-->
                                <div>
                                    <t t-esc="'.'*400"/>
                                </div>
                                <div>
                                    <t t-esc="'.'*400"/>
                                </div>
                                <div>
                                    <t t-esc="'.'*400"/>
                                </div>
                                <div>
                                    <t t-esc="'.'*400"/>
                                </div>
                                <div>
                                    <t t-esc="'.'*400"/>
                                </div>
                                <div>
                                    <t t-esc="'.'*400"/>
                                </div>
                                <div>
                                    <t t-esc="'.'*400"/>
                                </div>
                                <div>
                                    <t t-esc="'.'*400"/>
                                </div>
                                <div>
                                    <t t-esc="'.'*400"/>
                                </div>
                                <div>
                                    <t t-esc="'.'*400"/>
                                </div>
                                <div>
                                    <t t-esc="'.'*400"/>
                                </div>
                                <div>
                                    <t t-esc="'.'*400"/>
                                </div>
                                <div>
                                    <t t-esc="'.'*400"/>
                                </div>
                                <div>
                                    <t t-esc="'.'*400"/>
                                </div>
                                <div>
                                    <t t-esc="'.'*400"/>
                                </div>
                                <div>
                                    <t t-esc="'.'*400"/>
                                </div>
                            </div>
                        </div>

                    </div>
                </t>
            </t>
        </template>

        <template id="checkin_template">
            <t t-call="web.html_container">
                <t t-foreach="docs" t-as="o">
                    <t t-call="check_in_app.checkin_document" t-lang="o.partner.lang"/>
                </t>
            </t>
        </template>

