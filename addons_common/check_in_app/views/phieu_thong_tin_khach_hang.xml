        <report
                string="In phiếu tư vấn khách hàng"
                id="print_checkin_action"
                model="crm.check.in"
                report_type="qweb-pdf"
                name="check_in_app.checkin_template"
                file="check_in_app.checkin_template"
                paperformat="check_in_app.paperformat_checkin"
        />


        <!--        Checkin khách hàng chưa có Booking-->
        <template id="template_customer_information_checkin">
            <t t-call="web.external_layout">
                <t t-set="o" t-value="docs[0].with_context({'lang':lang})"/>
                <t t-set="o" t-value="o.with_user(1)"/>
                <div class="page" style="font-family:Times New Roman;font-size:19px;">

                    <div class="oe_structure"/>
                    <div class="row mt32" style="font-family:Times New Roman;font-size:17px;">
                        <div class="col-3">
                            <!--                            <img src="/crm_his_13/static/description/logo_kn.jpg" width="70%" height="auto"/>-->
                            <t t-if="o.company_id.brand_id.code == 'KN'">
                                <img src="/check_in_app/static/description/logo_kn.png" width="70%" height="auto"/>
                            </t>
                            <t t-elif="o.company_id.brand_id.code == 'PR'">
                                <img src="/check_in_app/static/description/logo_pr.png" width="70%" height="auto"/>
                            </t>
                            <t t-else=""/>
                        </div>
                        <div class="col-7">
                            <div>
                                <b>
                                    <span t-field="o.company_id.name"/>
                                </b>
                            </div>
                            <div>
                                <strong>Địa chỉ:</strong>
                                <span t-field="o.company_id.street"/>,
                                <span t-field="o.company_id.street2"/>,
                                <span t-field="o.company_id.city"/>
                            </div>
                            <!--                            <div>-->
                            <!--                                <strong>HOTLINE:</strong>-->
                            <!--                                <span t-field="o.company_id.phone"/>-->

                            <!--                            </div>-->
                            <div>
                                <strong>WEBSITE:</strong>
                                <span t-field="o.company_id.website"/>
                            </div>
                        </div>
                        <div class="col-2 text-center">
                            <img t-if="o.qr_checkin" t-att-src="image_data_uri(o.qr_checkin)" width="100"
                                 border="1"/>
                            <br/>
                            <i style="font-size:15px">QR Checkinnnn</i>
                        </div>

                    </div>
                    <div class="text-center" style="font-family:Times New Roman;font-weight:bolder;">
                        <div style="font-size: 24px;">PHIẾU THÔNG TIN NHÓM DỊCH VỤ TƯ VẤN</div>
                    </div>
                    <div class="col-12 text-right">
