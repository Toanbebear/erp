<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <record id="res_country_inherit" model="ir.ui.view">
            <field name="name">res.country.district.inherit</field>
            <field name="model">res.country.district</field>
            <field name="inherit_id" ref="base_unit_vn.res_country_district_form_view"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='name']" position="after">
                    <field name="code"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>