<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>

        <record id="view_company_zone_form_inherit" model="ir.ui.view">
            <field name="name">View Company Zone Form Inherit</field>
            <field name="model">res.company</field>
            <field name="inherit_id" ref="sci_brand.view_form_company_with_brand"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='brand_id']" position="after">
                    <field name="zone"/>
                </xpath>
            </field>
        </record>

    </data>
</odoo>
