<odoo>
    <data>
        <template id="template_service_information_sheet">
            <t t-call="web.external_layout">
                <t t-set="o" t-value="docs[0].with_context({'lang':docs[0].patient.partner_id.lang})"/>
                <t t-set="o" t-value="o.with_user(1)"/>
                <div class="page" style="font-family:Times New Roman;font-size:18px;">

                    <div class="oe_structure"/>
                    <div class="row mt32 mb32">
                        <div class="col-5">
                            <t t-if="o.institution.his_company.id == 12">
                                <img
                                 src="/shealth_all_in_one/static/img/logo_kn_218.png"
                                 width="100"
                                 border="1"/>
                            </t>
                            <t t-else="">
                                <img t-if="o.company_id.favicon"
                                 t-att-src="image_data_uri(o.institution.his_company.favicon)" width="60%" border="0"/><br/>
                            </t>
<!--                            <span t-field="o.institution.name" style="font-size:10px;"/>-->
                        </div>
                        <div class="col-7" style="font-size:16px;">
                            <div>
                                <t t-if="o.institution.his_company.id == 12">
                                    <span>CÔNG TY TNHH BVTM KANGNAM</span>
                                </t>
                                <t t-else="">
                                    <span t-field="o.institution.company_name"/>
                                </t>
                            </div>
                            <div>
                                <strong>Địa chỉ:</strong>
                                <span class="m-0" t-field="o.institution.street"/>,
                                <span class="m-0" t-field="o.institution.street2"/>,
                                <span class="m-0" t-field="o.institution.city"/>
                            </div>
                            <div>
                                <strong>HOTLINE:</strong>
                                <span t-field="o.institution.phone"/>

                            </div>
                            <div>
                                <strong>WEBSITE:</strong>
                                <span t-field="o.institution.website"/>
                            </div>
                        </div>

                    </div>
                    <div class="text-center" style="font-family:Times New Roman;font-weight:bolder;">
                        <div style="font-size: 25px;">PHIẾU THÔNG TIN DỊCH VỤ PHẪU THUẬT</div>
                    </div>
                    <!--                    <div class="text-center">-->
                    <!--                        <span>Mã Booking:</span>-->
                    <!--                        <b>-->
                    <!--                            <i t-field="o.booking_id.name"/>-->
                    <!--                        </b>-->
                    <!--                    </div>-->
                    <br/>
                    <div class="row col-12">
                        <div class="col-6">Họ và tên:
                            <strong>
                                <span t-field="o.patient.name"/>
                            </strong>
                        </div>
                        <div class="col-6">Mã Booking:
                            <b>
                                <i t-field="o.booking_id.name"/>
                            </b>
                        </div>
                    </div>
                    <div class="row col-12">
                        <div class="col-7">
                            Ngày/tháng/năm sinh - Tuổi:
                            <t t-if="o.patient.partner_id.birth_date">
                                <strong>
                                    <span t-field="o.patient.partner_id.birth_date"/>
                                </strong>
                            </t>
                            <t t-elif="o.patient.partner_id.year_of_birth">
                                <strong>
                                    <span t-field="o.patient.partner_id.year_of_birth"/>
                                </strong>
                            </t>
                            <t t-else="">
                            </t>
                            <t t-if="o.patient.partner_id.birth_date">
                                <strong>
                                    -
                                    <span t-field="o.patient.age"/>
                                </strong>
                            </t>
                        </div>
                        <div class="col-5">Nghề nghiệp:
                            <t t-if="o.booking_id.career">
                                <strong>
                                    <span t-field="o.booking_id.career"/>
                                </strong>
                            </t>
                            <t t-else="">
                                Chưa có thông tin
                            </t>

                        </div>

                    </div>

                    <div class="col-12">
                        Địa chỉ :
                        <t t-if="o.patient.partner_id.pass_port_address">
                            <strong>
                                <span t-field="o.patient.partner_id.pass_port_address"/>
                            </strong>
                        </t>
                        <t t-else="">
                            <strong>
                                <span t-field="o.patient.partner_id.street"/>,
                                <span t-field="o.patient.partner_id.district_id.name"/>,
                                <span t-field="o.patient.partner_id.state_id.name"/>,
                                <span t-field="o.patient.partner_id.country_id.name"/>
                            </strong>
                        </t>
                    </div>
                    <table class="table border border-dark text-center" style="font-size: 16px;">
                        <tr>
                            <th class="border border-dark align-middle">
                                <strong>STT</strong>
                            </th>
                            <th class="border border-dark align-middle">
                                <strong>Dịch vụ tư vấn</strong>
                            </th>
                            <th class="border border-dark align-middle">
                                <strong>Đơn vị xử lý</strong>
                            </th>
                            <th class="border border-dark align-middle">
                                <strong>Tư vấn viên</strong>
                            </th>
                        </tr>
                        <br/>
                        <t t-if="o.sale_order_id.order_line">
                            <t t-set="i" t-value="0"/>
                            <t t-foreach="o.sale_order_id.order_line" t-as="line">
                                <t t-set="i" t-value="i + 1"/>
                                <tr t-if="line">
                                    <td style="text-align:center;" class="border border-dark align-middle">
                                        <span t-esc="i"/>
                                    </td>
                                    <td style="text-align:left;" class="border border-dark align-middle">
                                        <span t-field="line.product_id.name"/>
                                        <br/>

                                    </td>
                                    <td style="text-align:center;" class="border border-dark align-middle">
                                        <span t-field="line.uom_price"/>
                                    </td>
                                    <td style="text-align:left;" class="border border-dark align-middle">
                                        -
                                        <span style="font-size: 12px" t-field="line.crm_line_id.consultants_1.name"/>
